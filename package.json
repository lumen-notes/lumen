  "scripts": {
    "dev": "vite dev",
    "dev:netlify": "netlify dev",
    "dev:storybook": "storybook dev -p 6006",
    "build": "tsc && vite build",
    "build:service-worker": "workbox generateSW workbox.config.cjs",
    "build:storybook": "storybook build",
    "postbuild": "npm run build:service-worker",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:storybook": "test-storybook",
    "test:storybook:watch": "test-storybook --watch",
    "test:storybook:ci": "concurrently -k -s first -n \"server,test\" -c \"magenta,blue\" \"yarn build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook\"",
